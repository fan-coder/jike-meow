{
  "name": "Jike Web QR",
  "version": "0.0.3",
  "author": "coder_ysj",
  "description": "即刻网页版二维码登录插件",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "<all_urls>"
  ],
  "incognito": "not_allowed",
  "offline_enabled": false,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "js": [
        "scripts/axios.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/jk16.png",
      "32": "images/jk32.png",
      "48": "images/jk48.png",
      "128": "images/jk128.png"
    }
  },
  "icons": {
    "16": "images/jk16.png",
    "32": "images/jk32.png",
    "48": "images/jk48.png",
    "128": "images/jk128.png"
  },
  "manifest_version": 2
}