{
  "name": "Jike Web QR",
  "version": "0.0.2",
  "author": "coder_ysj",
  "description": "即刻二维码登录效果演示插件，欢迎在即刻 App 中搜索 “coder_ysj” 关注我",
  "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
  "incognito": "not_allowed",
  "offline_enabled": false,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/jk16.png",
      "32": "images/jk32.png",
      "48": "images/jk48.png",
      "128": "images/jk128.png"
    }
  },
  "icons": {
    "16": "images/jk16.png",
    "32": "images/jk32.png",
    "48": "images/jk48.png",
    "128": "images/jk128.png"
  },
  "manifest_version": 2
}