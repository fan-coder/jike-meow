{
  "name": "即刻喵",
  "version": "1.0.2",
  "author": "coder_ysj",
  "description": "可独立使用的非官方「即刻」通知浏览插件，并为官方网页版（web.okjike.com）提供一键登录功能。",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "background",
    "<all_urls>"
  ],
  "incognito": "not_allowed",
  "offline_enabled": false,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "http://web.okjike.com/*",
        "https://web.okjike.com/*"
      ],
      "js": [
        "scripts/axios.min.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "scripts/axios.min.js",
      "scripts/socket.io.dev.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/jk16.png",
      "32": "images/jk32.png",
      "48": "images/jk48.png",
      "128": "images/jk128.png"
    }
  },
  "icons": {
    "16": "images/jk16.png",
    "32": "images/jk32.png",
    "48": "images/jk48.png",
    "128": "images/jk128.png"
  },
  "manifest_version": 2
}